//creating my initals ss using the printline function //

public class YourInitials {
    //main function//
    public static void main(String[] args) {
   System.out.println(" sssss     sssss ");
   System.out.println("s     s   s     s");
   System.out.println("s         s      ");
   System.out.println(" sssss     sssss");
   System.out.println("      s         s");
   System.out.println("s     s   s     s");
   System.out.println(" sssss     sssss");

    }
}
